<Window x:Class="VEZA_Internal.Main_Wind"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VEZA_Internal"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button

            Command="{Binding PlaceInstance}"
            Content="Установить элемент" Height="20" Margin="400,108,267,306"/>
        <Button 
            Command="{Binding PlaceDimension}"
            Content="Установить размер" Margin="400,154,267,260" Height="20"/>
        <Button 
            Command="{Binding DeleteViewPort}"
            Content="DeleteViewPort" Height="20" Margin="400,199,267,215" d:LayoutOverrides="VerticalAlignment"/>
        <Button 
            Command="{Binding PrintPDF}"
            Content="PrintPDF" Margin="400,236,267,178" Height="20"/>
        <Button 
            Command="{Binding ExportDWG}"
            Content="Export to DWG" Margin="400,276,267,138" Height="20"/>
        <Button 
            Command="{Binding FamAnalitic}"
            Content="FamAnalitic" Margin="400,318,267,96" Height="20"/>

    </Grid>
</Window>

<Window x:Class="VEZA_Internal.Main_Wind"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VEZA_Internal"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button

            Command="{Binding PlaceInstance}"
            Content="Установить элемент" Height="20" Margin="392,166,281,248" d:LayoutOverrides="HorizontalAlignment"/>
        <Button 
            Command="{Binding PlaceDimension}"
            Content="Установить размер" Margin="392,212,275,202" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" Height="20"/>

    </Grid>
</Window>
